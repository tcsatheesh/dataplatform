{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "resources": {
            "value": [
                {
                    "type": "appservice",
                    "resourceType": "appservices",
                    "resourceGroupTypeRef": "web",
                    "name": "{0}{1}{2}app01",
                    "location": "North Europe",
                    "templateFileName": "appservice.json",
                    "parameterFileName": "appservice.parameters.json",
                    "parameters": [
                        {
                            "name": "hostingPlanName",
                            "type": "format",
                            "value": "{0}{1}{2}pln01"
                        },
                        {
                            "name": "appinsightsname",
                            "type": "format",
                            "value": "{0}{1}{2}ain01"
                        },
                        {
                            "name": "sku",
                            "type": "value",
                            "value": "Standard"
                        },
                        {
                            "name": "skuCode",
                            "type": "value",
                            "value": "S1"
                        },
                        {
                            "name": "workerSize",
                            "type": "value",
                            "value": "0"
                        },
                        {
                            "name": "serverFarmResourceGroup",
                            "type": "reference",
                            "ref": {
                                "resourceType": "resourcegroups",
                                "typeFilter": "app",
                                "property": "name"
                            }
                        },
                        {
                            "name": "subscriptionId",
                            "type": "reference",
                            "ref": {
                                "resourceType": "projects",
                                "typeFilter": "subscription",
                                "property": "id"
                            }
                        },
                        {
                            "name": "sslCertName",
                            "type": "value",
                            "value": "app.rubycom.info"
                        },
                        {
                            "name": "sslCertHostName",
                            "type": "value",
                            "value": "app.rubycom.info"
                        },
                        {
                            "name": "sslCertData",
                            "type": "keyvault",
                            "ref": {
                                "resourceType": "keyvaults",
                                "typeFilter": "appkeyvault",
                                "secretName": "app-rubycom-info-certificate"
                            }
                        },
                        {
                            "name": "sslCertPassword",
                            "type": "keyvault",
                            "ref": {
                                "resourceType": "keyvaults",
                                "typeFilter": "appkeyvault",
                                "secretName": "app-rubycom-info-certificate-password"
                            }
                        },
                        {
                            "name": "sslCertThumbprint",
                            "type": "reference",
                            "ref": {
                                "resourceType": "keyvaultsecrets",
                                "typeFilter": "certificate",
                                "subtypeFilter": "webappcertificate",
                                "property": "certificateThumbprint"
                            }
                        }                        
                    ]
                }
            ]
        }
    }
}